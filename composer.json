{
    "name": "generation-2/store",
    "description": "Store service of the Generation 2 bundle",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Manuele Vaccari",
            "email": "manuele.vaccari@gmail.com",
            "homepage": "https://github.com/D3strukt0r",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "manuele.vaccari@gmail.com",
        "source": "https://github.com/D3strukt0r/generation-2-store-api",
        "issues": "https://github.com/D3strukt0r/generation-2-store-api/issues",
        "wiki": "https://github.com/D3strukt0r/generation-2-store-api/wiki"
    },
    "require": {
        "php": "^7.2.5",
        "ext-SimpleXML": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "braintree/braintree_php": "^5.1",
        "d3strukt0r/oauth2-generation-2": "dev-master",
        "google/recaptcha": "^1.2",
        "knpuniversity/oauth2-client-bundle": "^2.1",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^6.0",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.1.*",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/string": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/D3strukt0r/oauth2-generation-2" }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
